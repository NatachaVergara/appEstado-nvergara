{
  "expo": {
    "name": "busco-libro-app",
    "slug": "busco-libro-app",
    "version": "1.0.0",   
    "orientation": "portrait",
    "icon": "./assets/img/closeBook2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/closeBook2.png",
      "resizeMode": "contain",
      "backgroundColor": "##a31f05"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/closeBook2.png",
        "backgroundColor": "##a31f05"
      }
    },
    "web": {
      "favicon": "./assets/img/closeBook2.png"
    }
  }
}
